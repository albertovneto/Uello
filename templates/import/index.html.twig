{% block body %}
<h1>Lista de CSV's Importados</h1>
    <hr>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Cod.</th>
                <th>Nome</th>
                <th>Data de Importação</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
        {% for import in imports %}
            <tr>
                <td>import.id</td>
                <td>import.name</td>
                <td>import.data</td>
                <td>
                    <a class="btn btn-sm btn-warning" href=""><i class="fa fa-eye"></i></a>
                    <a class="btn btn-sm btn-danger" href=""><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}